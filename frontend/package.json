{
  "name": "rockfall-dashboard",
  "version": "1.0.0",
  "description": "AI-Based Rockfall Prediction Dashboard for Open-Pit Mines",
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "express": "^4.18.2",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "chmod +x ./node_modules/.bin/react-scripts || echo 'chmod not available'",
    "build": "node build.js",
    "build:original": "CI=false ./node_modules/.bin/react-scripts build",
    "serve": "serve -s build -l $PORT",
    "start:prod": "serve -s build -p $PORT",
    "start:server": "node server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}